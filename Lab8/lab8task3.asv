close all; clc; clear

% import data
d = importdata('xy_data.txt');
x = d(1,1:end);
y = d(2,1:end);
% plot data
plot(x,y,'ko')
title('y against x')
xlabel('x')
ylabel('y')

% fit 7th deg polynomial
p = polyfit(x,y,7);
domx = min(x):0.1:max(x);
hold on
plot(domx, polyval(p,domx))

% separate into 2 sets
x1 = x(logical())


legend('y','7th order curve of best fit','Location', 'best')